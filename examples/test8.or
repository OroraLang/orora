MAX := 100
i := 0
\begin{while}{1}
  i := i + 1
  j := 0
  c := ""
  k := 0
  \begin{while}{1}
    k := k + 1
    \begin{if}{k = MAX - i + 1}
      break
    \end{if}
    c := c * " "
  \end{while}
  \begin{while}{1}
    j := j + 1
    c := c * "*"
    \begin{if}{i = j}
      print(c)
      break
    \end{if}
  \end{while}
  \begin{if}{i = MAX}
    break
  \end{if}
  print("\n")
\end{while}
